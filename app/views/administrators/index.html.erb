<p id="notice"><%= notice %></p>

<h1>Administrator</h1>

<table>
  <thead>
    <tr>
      <th>Name</th>
      <th colspan="3"></th>
    </tr>
  </thead>

  <tbody>
    <% @administrators.each do |ad| %>
      <tr>
        <td><%= ad.name %></td>
        <td><%= link_to 'Show', ad %></td>
        <td><%= link_to 'Edit', edit_administrator_path(ad) %></td>
        <td><%= link_to 'Destroy', ad, method: :delete, data: { confirm: 'Are you sure?' } %></td>
      </tr>
    <% end %>
  </tbody>
</table>

<br>

<%= link_to 'New administrator', new_administrator_path %>
